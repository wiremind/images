# Slack
SLACK_BOT_TOKEN=xoxb-your-token
SLACK_SIGNING_SECRET=your-signing-secret
SLACK_APP_TOKEN=xapp-your-app-token
SLACK_TRIGGER_EMOJI=slackagent
SLACK_TRIGGER_REACTION=eyes
SLACK_AGENT_ACK_REACTION=white_check_mark

# HTTP server
HOST=0.0.0.0
PORT=3000

# A2A (agent endpoint)
SLACKAGENT_A2A_URL=https://your-a2a-agent.example.com
#SLACKAGENT_A2A_HEADERS_JSON={"Authorization":"Bearer your-token"}
SLACKAGENT_A2A_TRANSPORTS=jsonrpc,http+json
SLACKAGENT_A2A_TIMEOUT_S=600
SLACKAGENT_A2A_POLL_INTERVAL_S=0.5
#SLACKAGENT_A2A_POLL_TIMEOUT_S=
SLACKAGENT_A2A_USE_EXTENDED_CARD=false
SLACKAGENT_A2A_INSECURE=false
